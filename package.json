{
  "name": "patient-safety-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "prettier": "^1.5.3"
  },
  "scripts": {
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
